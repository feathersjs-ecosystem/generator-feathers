    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@feathersjs/primus": "^4.5.11",
    "@feathersjs/socketio": "^4.5.11",
    "body-parser": "^1.19.0",
    "cassanknex": "^1.20.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "debug": "^4.3.1",
    "eslint": "^7.25.0",
    "express-cassandra": "^2.6.0",
    "feathers-cassandra": "^3.5.5",
    "feathers-knex": "^7.1.1",
    "feathers-memory": "^4.1.0",
    "feathers-mongodb": "^6.3.0",
    "feathers-mongoose": "^8.3.3",
    "feathers-nedb": "^5.1.0",
