    "feathers-knex": "^3.3.0",
    "feathers-memory": "^2.1.3",
    "feathers-mongodb": "^3.2.0",
    "feathers-mongoose": "^6.1.2",
    "feathers-nedb": "^3.1.0",
    "feathers-rethinkdb": "^0.5.2",
    "feathers-sequelize": "^3.1.2",
    "helmet": "^3.12.1",
    "jshint": "^2.9.5",
    "mocha": "^5.2.0",
    "mongodb": "^3.1.0",
    "mongoose": "^5.2.0",
    "nedb": "^1.8.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "rethinkdbdash": "^2.3.31",
    "sequelize": "^4.38.0",
    "serve-favicon": "^2.5.0",
